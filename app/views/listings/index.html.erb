<div class="row">
  <h1>Find the office space you need</h1>

  <% @listings.each do |listing| %>
    <div class="col-md-4">
        <div class="thumbnail">
        <%= image_tag listing.image.url(:medium) %>
        <div class="caption">
        <h3><%= listing.name %></h3>
        <p> <strong> <%= listing.price %> </strong> SAR per workday</p>
        <p><%= listing.city %></p>
        <%= link_to 'Show', listing,  class: "btn btn-link"  %>
        <%= link_to 'Edit', edit_listing_path(listing),  class: "btn btn-link" %>
    <%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' },  class: "btn btn-link"  %>
      </div>
     </div>
   </div>
<% end %>
</div>
<hr>
<div class="row">
<div class="col-md-4">
<%= link_to 'Add a Workspace', new_listing_path, class: "btn btn-danger" %> </div>
</div>