  <div class="jumbotron">
  <h3>No office, no problem!</h3>
  <p>Find and book available workspaces around you.</p>
</div>

<div class="row">
</br>
  <% @listings.each do |listing| %>
    <div class="col-md-4">
        <div class="thumbnail">
        <%= link_to image_tag(listing.image.url(:medium) ), listing %>
        <div class="caption">
        <h3><span class="label label-primary"><%= listing.name %></span></h3>
        <p> Add by <strong><%= listing.user.name %></strong>  </p>
        <p> <strong> <%= listing.price %> </strong> SAR per workday</p>
        <p><%= listing.city %></p>
      
      </div>
     </div>
   </div>
<% end %>
</div>

<div class="row">
<div class="col-md-12">
      <% if user_signed_in? %>
           <%= link_to 'Add a Workspace', new_listing_path, class: "btn btn-danger center-block" %>
      <% end %>
    </div>
</div>
<hr>
